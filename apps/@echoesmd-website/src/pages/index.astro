// Sort by state
// 	Backlog
// 	In Progress
// 	Done
// Then sort by priority within each state
// 	High
// 	Medium
// 	Low

const backlog = issues.filter((issue) => issue.state === "Backlog");
const inProgress = issues.filter((issue) => issue.state === "In Progress");
const done = issues.filter((issue) => issue.state === "Done");
const cancelled = issues.filter((issue) => issue.state === "Cancelled");
const todo = issues.filter((issue) => issue.state === "Todo");

const getColor = (state: string) => {
  switch (state) {
    case "Backlog":
      return "text-red-500";
    case "In Progress":
      return "text-yellow-500";
    case "Done":
      return "text-green-500";
    default:
      return "text-gray-500";
  }
};

const isCloudflareWorker = import.meta.env.CLOUDFLARE_WORKER_ENV === 'true';

